<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomFontTextView">
        <attr name="textFont" format="enum">
            <enum name="Roboto_Black" value="0" />
            <enum name="Roboto_BlackItalic" value="1" />
            <enum name="Roboto_Bold" value="2" />
            <enum name="Roboto_BoldItalic" value="3" />
            <enum name="Roboto_Italic" value="4" />
            <enum name="Roboto_Light" value="5" />
            <enum name="Roboto_LightItalic" value="6" />
            <enum name="Roboto_Medium" value="7" />
            <enum name="Roboto_MediumItalic" value="8" />
            <enum name="Roboto_Regular" value="9" />
            <enum name="Roboto_Thin" value="10" />
            <enum name="Roboto_ThinItalic" value="11" />
            <enum name="RobotoCondensed_Bold" value="12" />
            <enum name="RobotoCondensed_BoldItalic" value="13" />
            <enum name="RobotoCondensed_Italic" value="14" />
            <enum name="RobotoCondensed_Light" value="15" />
            <enum name="RobotoCondensed_LightItalic" value="16" />
            <enum name="RobotoCondensed_Regular" value="17" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LoadingImageView">
        <attr name="anim_auto_start" format="boolean" />
        <attr name="mask_color" format="color" />
    </declare-styleable>

    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_colorRipple" format="color" />
        <attr name="fab_shadow" format="boolean" />
        <attr name="fab_type" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MaterialSearchView">
        <attr name="leftDrawableType" format="enum">
            <enum name="menu" value="0" />
            <enum name="back" value="1" />
            <enum name="search" value="2" />
            <enum name="avatar" value="3" />
        </attr>
        <attr name="hintText" format="string" />
        <attr name="layout_marginTop" format="dimension"/>
        <attr name="layout_marginBottom" format="dimension"/>
        <attr name="layout_marginLeft" format="dimension"/>
        <attr name="layout_marginRight" format="dimension"/>
    </declare-styleable>

